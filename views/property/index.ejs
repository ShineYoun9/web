

<% include ../sample.ejs %>

<% property.forEach( function(model) { %>
<div class="col-sm-6 col-md-6">
<table border="1"  style="margin:0px auto">
	<div style="display:inline">
<tr>
<td colspan="3" class="text-center">
<h5>
<a href="clearview/<%=model.id%>"><%=model.estatetitle%></a>
</h5>
</td>
</tr>
<tr>
<td>
<img src="<%=model.url%>" width="150" height="150" class="img-rounded center-block" />
</td>
<td width="5"></td>
<td>
<div class="panel panel-default">
<div class="panel-heading">
<h3 class="panel-title">
Estate:<small><%=model.estate%></small>
</h3>
</div>
<div class="panel-body">
Gross Area:<small><%=model.area%></small>
</div>
<div class="panel-footer">
Rent:<small><%=model.rent%></small>
</div>
</div>
</td>
</tr>
</div>
</table>
</div>
<% }); %>



	<script src="http://cdn.static.runoob.com/libs/jquery/2.1.1/jquery.min.js"></script>
	<script src="http://cdn.static.runoob.com/libs/bootstrap/3.3.7/js/bootstrap.min.js">
	</script>