<% include ../sample.ejs %>


<table class="table">
	
<% property.forEach( function(model) { %>
<%var m=model.id%>
<tr><td><a href="/property/clearview/<%=m%>"><%=model.estatetitle%></a></td>
	<td><%=model.estate%></td>
	<td><a href="/property/showIntMembers/<%=m%>">Interested Members</a></td>
    <td><a href="update/<%=m%>">Edit</a></td>
<td><a onclick="deletedata(<%=m%>)">Delete</a></td></tr>
<% }); %>
</table>

<script >
function deletedata(id )
{ var s = confirm("Deleteï¼Ÿ");
	if(s == true){
		var d = new XMLHttpRequest();
		d.open("POST", "/property/delete/" + id,true);
		d.onload = function(m){
			alert(this.responseText);
			location.reload(true);
		};
		d.send();
		}
	else {
		alert("cancel");

	}
}
</script>




