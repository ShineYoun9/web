


<% include ../sample.ejs %>

    
<table class="table">
<% property.forEach( function(model) { %>
<tr><td><%=model.estatetitle%></td><td><%=model.estate%></td><td><a href="showIntMembers/<%=model.id%>">Interested Members</a></td>
<td><a href="/property/update/<%=model.id%>">Edit</a></td>
<td><a onclick="deletedata(<%=model.id%>)">Delete</a></td></tr>
<% }); %>
</table>
<script >
function deletedata(id )
{ var s = confirm("Deleteï¼Ÿ");
